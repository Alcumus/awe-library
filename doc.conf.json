{
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "include": [
      "bind.js",
      "document-type-context.js",
      "fields.js",
      "generic-list-entry.js",
      "lookup-fields.js",
      "query.js",
      "question-typedef.js",
      "use-types.js",
      "utils.js",
      "../../common/use-async/index.js",
      "../../common/offline-data-service/index.js",
      "../../common/offline-data-service/records.js",
      "../../common/menu/index.js",
      "../../common/process/index.js",
      "../../common/sortable/index.js",
      "../../common/use-event/index.js",
      "runtime/query.js",
      "runtime/utils.js",
      "runtime/navigate.js",
      "runtime/create-document.js",
      "runtime/app-urls.js",
      "runtime/button-style.js",
      "runtime/records.js",
      "runtime/contexts.js"
    ]
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "encoding": "utf8",
    "template": "./node_modules/docdash",
    "readme": "./README.md"
  },
  "docdash": {
    "search": true,
    "typedefs": true,
    "sort": true,
    "meta": {
      "title": "awe-library"
    },
    "openGraph": {
      "title": "awe-library",
      "type": "docs",
      "site_name": "awe-library"
    }
  }
}
